public class find{
  
  public static int findMin(int[] arr){
    
    int min = arr[0];
  
      for(int i=0; i < arr.length-1; j++)
        if(arr[i] < arr[j]){
        min = arr[i]
      }
    }
 
  
   public static int findMax(int[] arr){
  nt maxValue = array[0];
    for (int i = 1; i < array.length; i++) {
        if (array[i] > maxValue) {
            maxValue = array[i];
        }
    }
    return maxValue;
  }
   
