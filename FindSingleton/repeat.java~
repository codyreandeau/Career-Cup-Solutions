public class repeat {
  
  public static void main(String[] args) {
    
    int[] arr = {1,2,1,2,3,4,3,4,5,7,6,5,6};
    boolean x = false;
    int single = 0;
    int j;
    
    for(int i=0; i <= arr.length; i++){
      if(x == false){
        single = arr[i-1];
      }
      x = false;
      for(j=0; j <= arr.length; i++){
        if(arr[i] == arr[j]){
          x=true;
        }
      }
    }
    
    System.out.println(single);
    
  }
}